@{
    ViewData ["Title"] = "Lancamento";
}

@model IEnumerable<jrintegracaoipm.Models.Lancamento>



<table class="table table-striped table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.CliCodigo);
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LoaAno);
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LanValor);
            </th>

        </tr>

    </thead>
    <tbody>
        @foreach (var item in Model)
            {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.CliCodigo)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LoaAno)

                <td>
                    @Html.DisplayFor(modelItem => item.LanValor)
            </tr>
            }

    </tbody>

</table>
